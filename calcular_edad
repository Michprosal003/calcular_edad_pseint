Algoritmo sin_titulo
	
	Escribir  "Ingresa el valor de año actual:";
    Leer año_actual;
	
	
    Escribir  "Ingresa el valor de año de nacimiento:";
    Leer año_de_nacimiento;
	
	
    Escribir  "Ingresa el valor de dia actual:";
    Leer dia_actual;
	
	
    Escribir  "Ingresa el valor de dia de nacimiento:";
    Leer dia_de_nacimiento;
	
	
    Escribir  "Ingresa el valor de mes actual:";
    Leer mes_actual;
	
	
    Escribir  "Ingresa el valor de mes de nacimiento:";
	
    Leer mes_de_nacimiento;
	
	
    edad <- año_actual-año_de_nacimiento;
	
	
    Si mes_de_nacimiento>mes_actual O (mes_de_nacimiento = mes_actual Y dia_de_nacimiento>dia_actual) Entonces
        edad <- edad-1;
		
		
    FinSi
    Escribir "Valor de edad: ", edad;
	
FinAlgoritmo
